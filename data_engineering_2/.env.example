# Environment variables for Urban Mobility Pipeline

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC=urban-mobility-events

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=mobility
DB_USER=postgres
DB_PASSWORD=postgres

# API Configuration
PORT=8000

# Producer Configuration
PRODUCER_RATE=10  # events per second
PRODUCER_DURATION=  # leave empty for infinite

# Consumer Configuration
CONSUMER_GROUP_ID=mobility-consumer-group
